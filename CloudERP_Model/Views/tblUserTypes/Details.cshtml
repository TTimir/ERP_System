@model DatabaseAccess.tblUserType

@{
    string controllerName = "tblUserTypes";
    ViewBag.ControllerName = controllerName;
    string actionaName = "Index";
    ViewBag.ActionaName = actionaName;
    ViewBag.MainTitle = "All User Types";
    ViewBag.Title = "User Type Details";
}

<div class="row clearfix">
    <div class="col-lg-12">
        <div class="card">
            <div class="body">
                <div>
                    <dl class="row">
                        <dt class="col-sm-3 font-weight-bold">
                            @Html.DisplayNameFor(model => model.UserType)
                        </dt>
                        <dd class="col-sm-9">
                            @Html.DisplayFor(model => model.UserType)
                        </dd>
                    </dl>
                </div>

                <p>
                    <a href="@Url.Action("Edit", "tblUserTypes", new { id = Model.UserTypeID })" class="btn btn-primary ml-3">
                        Edit info
                    </a>
                    <a href="@Url.Action("Index", "tblUserTypes")" class="btn btn-default">
                        <i class="icon-arrow-left mr-2"></i> Back
                    </a>
                </p>

            </div>
        </div>
    </div>
</div>